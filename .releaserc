{
  "branches": [
    "master",
    "next",
    {
      "name": "beta",
      "prerelease": true
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "conventionalcommits",
        "releaseRules": [
          {
            "breaking": true,
            "release": "major"
          },
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          },
          {
            "type": "revert",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": "minor"
          },
          {
            "type": "style",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "test",
            "release": "patch"
          },
          {
            "type": "build",
            "release": "patch"
          },
          {
            "type": "ci",
            "release": "patch"
          },
          {
            "type": "chore",
            "release": false
          }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          "target/**/*.{msi,exe,app,dmg,rpm,deb,AppImage}"
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING"
          ]
        },
        "writerOpts": {
          "commitsSort": [
            "subject",
            "scope"
          ]
        },
        "presetConfig": {
          "types": [
            {
              "type": "feat",
              "section": "ðŸ• Features"
            },
            {
              "type": "feature",
              "section": "ðŸ• Features"
            },
            {
              "type": "fix",
              "section": "ðŸ› Bug Fixes"
            },
            {
              "type": "perf",
              "section": "ðŸ”¥ Performance Improvements"
            },
            {
              "type": "revert",
              "section": "â© Reverts"
            },
            {
              "type": "docs",
              "section": "ðŸ“ Documentation"
            },
            {
              "type": "style",
              "section": "ðŸŽ¨ Styles"
            },
            {
              "type": "refactor",
              "section": "ðŸ§‘â€ðŸ’» Code Refactoring"
            },
            {
              "type": "test",
              "section": "âœ… Tests"
            },
            {
              "type": "build",
              "section": "ðŸ¤– Build System"
            },
            {
              "type": "ci",
              "section": "ðŸ” Continuous Integration"
            }
          ]
        }
      }
    ],
    [
      "@semantic-release/exec",
      {
        "successCmd": "echo v${nextRelease.version} > VERSION"
      }
    ]
  ]
}
